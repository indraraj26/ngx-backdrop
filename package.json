{
  "name": "backdrop",
  "version": "0.0.3",
  "author": "indraraj26",
  "license": "MIT",
  "homepage": "https://github.com/indraraj26/ngx-backdrop",
  "bugs": {
    "url": "https://github.com/indraraj26/ngx-backdrop/issues"
  },
  "keywords": [
    "backdrop",
    "ng-backdrop",
    "ngx-backdrop",
    "angular-backdrop"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:indraraj26/ngx-backdrop.git"
  },
  "description": "This provides backdrop component with Props and Event to manage backdrop",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dist:test": "angular-http-server --path dist/backdrop --open",
    "build-lib": "ng build ngx-backdrop",
    "add-lib": "npm i projects/ngx-backdrop/dist",
    "lib": "npm run build-lib && npm uninstall @indraraj26/ngx-backdrop && npm run add-lib",
    "ngh:build": "ng build --base-href https://indraraj26.github.io/ngx-backdrop/ ",
    "ngh:deploy": "ngh --message=deploy-1 --dir=dist/backdrop --branch=gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@indraraj26/ngx-backdrop": "file:projects/ngx-backdrop/dist",
    "ngx-autosize": "^1.8.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "angular-http-server": "^1.10.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.1.0",
    "typescript": "~4.3.2"
  }
}
